{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "hello-world-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/helloWorld/HelloWorldWebPart.js",
          "manifest": "./src/webparts/helloWorld/HelloWorldWebPart.manifest.json"
        }
      ]
    },
    "employee-master-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/employeeMaster/EmployeeMasterWebPart.js",
          "manifest": "./src/webparts/employeeMaster/EmployeeMasterWebPart.manifest.json"
        }
      ]
    },
    "dealer-information-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dealerInformation/DealerInformationWebPart.js",
          "manifest": "./src/webparts/dealerInformation/DealerInformationWebPart.manifest.json"
        }
      ]
    },
    "create-query-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/createQuery/CreateQueryWebPart.js",
          "manifest": "./src/webparts/createQuery/CreateQueryWebPart.manifest.json"
        }
      ]
    },
    "search-webpart-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/searchWebpart/SearchWebpartWebPart.js",
          "manifest": "./src/webparts/searchWebpart/SearchWebpartWebPart.manifest.json"
        }
      ]
    },
    "upload-information-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/uploadInformation/UploadInformationWebPart.js",

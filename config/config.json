    "get-share-point-listto-grid-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/getSharePointListtoGrid/GetSharePointListtoGridWebPart.js",
          "manifest": "./src/webparts/getSharePointListtoGrid/GetSharePointListtoGridWebPart.manifest.json"
        }
      ]
    },
    "employee-details-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/employeeDetails/EmployeeDetailsWebPart.js",
          "manifest": "./src/webparts/employeeDetails/EmployeeDetailsWebPart.manifest.json"
        }
      ]
    },
    "assesment-task-one-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/assesmentTaskOne/AssesmentTaskOneWebPart.js",
          "manifest": "./src/webparts/assesmentTaskOne/AssesmentTaskOneWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "HelloWorldWebPartStrings": "lib/webparts/helloWorld/loc/{locale}.js",
    "EmployeeMasterWebPartStrings": "lib/webparts/employeeMaster/loc/{locale}.js",
    "DealerInformationWebPartStrings": "lib/webparts/dealerInformation/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "CreateQueryWebPartStrings": "lib/webparts/createQuery/loc/{locale}.js",
    "SearchWebpartWebPartStrings": "lib/webparts/searchWebpart/loc/{locale}.js",
    "UploadInformationWebPartStrings": "lib/webparts/uploadInformation/loc/{locale}.js",
    "GetSharePointListtoGridWebPartStrings": "lib/webparts/getSharePointListtoGrid/loc/{locale}.js",
    "EmployeeDetailsWebPartStrings": "lib/webparts/employeeDetails/loc/{locale}.js",
    "AssesmentTaskOneWebPartStrings": "lib/webparts/assesmentTaskOne/loc/{locale}.js"
